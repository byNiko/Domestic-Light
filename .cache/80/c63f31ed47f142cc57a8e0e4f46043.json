ine":120,"column":4},"generated":{"line":1,"column":1126}},{"source":"src/convertColors.js","name":"rgb","original":{"line":120,"column":12},"generated":{"line":1,"column":1128}},{"source":"src/convertColors.js","original":{"line":120,"column":16},"generated":{"line":1,"column":1130}},{"source":"src/convertColors.js","name":"rgb","original":{"line":120,"column":20},"generated":{"line":1,"column":1133}},{"source":"src/convertColors.js","original":{"line":120,"column":24},"generated":{"line":1,"column":1135}},{"source":"src/convertColors.js","name":"rgb","original":{"line":120,"column":28},"generated":{"line":1,"column":1138}},{"source":"src/convertColors.js","original":{"line":120,"column":32},"generated":{"line":1,"column":1140}},{"source":"src/convertColors.js","name":"concat","original":{"line":120,"column":35},"generated":{"line":1,"column":1144}},{"source":"src/convertColors.js","name":"concat","original":{"line":120,"column":35},"generated":{"line":1,"column":1150}},{"source":"src/convertColors.js","name":"concat","original":{"line":120,"column":35},"generated":{"line":1,"column":1153}},{"source":"src/convertColors.js","name":"normalizeStrength","original":{"line":122,"column":7},"generated":{"line":1,"column":1160}},{"source":"src/convertColors.js","name":"strength","original":{"line":122,"column":25},"generated":{"line":1,"column":1162}},{"source":"src/convertColors.js","original":{"line":123,"column":3},"generated":{"line":1,"column":1165}},{"source":"src/convertColors.js","name":"exports","original":{"line":124,"column":2},"generated":{"line":1,"column":1172}},{"source":"src/convertColors.js","name":"wavelengthToHSL","original":{"line":124,"column":2},"generated":{"line":1,"column":1180}},{"source":"src/convertColors.js","name":"wavelengthToHSL","original":{"line":124,"column":2},"generated":{"line":1,"column":1196}}],"sources":{"src/convertColors.js":"// https://en.wikipedia.org/wiki/Visible_spectrum\nexport const colorList = {\n  F1_415: \"Violet\",\n  F2_445: \"Dark Violet\",\n  F3_480: \"Blue\",\n  F4_515: \"Green\",\n  A5_VIS1: \"A5_VIS1\",\n  A6_NIR1: \"A6_NIR1\",\n  F5_555: \"Dark Green\",\n  F6_590: \"Deep Yellow\",\n  F7_630: \"Light Red\",\n  F8_680: \"Red\",\n  F9_VIS: \"F9_VIS\",\n  F10_NIR: \"F10_NIR\"\n};\n\nfunction rgb2hue(r, g, b) {\n  r /= 255;\n  g /= 255;\n  b /= 255;\n  let segment;\n  let shift;\n  let max = Math.max(r, g, b);\n  let min = Math.min(r, g, b);\n  let c = max - min;\n  let hue;\n  if (c === 0) {\n    hue = 0;\n  } else {\n    switch (max) {\n      case r:\n        segment = (g - b) / c;\n        shift = 0 / 60; // R° / (360° / hex sides)\n        if (segment < 0) {\n          // hue > 180, full rotation\n          shift = 360 / 60; // R° / (360° / hex sides)\n        }\n        hue = segment + shift;\n        break;\n      case g:\n        segment = (b - r) / c;\n        shift = 120 / 60; // G° / (360° / hex sides)\n        hue = segment + shift;\n        break;\n      case b:\n        segment = (r - g) / c;\n        shift = 240 / 60; // B° / (360° / hex sides)\n        hue = segment + shift;\n        break;\n      default:\n        segment = (r - g) / c;\n        shift = 240 / 60; // B° / (360° / hex sides)\n        hue = segment + shift;\n    }\n  }\n  return hue * 60; // hue is in [0,6], scale it up\n}\n\nlet normalizeStrength = (num) =>\n  num > 99 && num < 1601 ? (num / 1600) * 100 : 50;\n\nlet wavelengthToRGB = function (wavelength) {\n  let R = 0;\n  let G = 0;\n  let B = 0;\n\n  // core translation into color\n  if (wavelength >= 380 && wavelength <= 440) {\n    R = (-1 * (wavelength - 440)) / (440 - 380);\n    G = 0;\n    B = 1;\n  } else if (wavelength > 440 && wavelength <= 490) {\n    R = 0;\n    G = (wavelength - 440) / (490 - 440);\n    B = 1;\n  } else if (wavelength > 490 && wavelength <= 510) {\n    R = 0;\n    G = 1;\n    B = (-1 * (wavelength - 510)) / (510 - 490);\n  } else if (wavelength > 510 && wavelength <= 580) {\n    R = (wavelength - 510) / (580 - 510);\n    G = 1;\n    B = 0;\n  } else if (wavelength > 580 && wavelength <= 645) {\n    R = 1;\n    G = (-1 * (wavelength - 645)) / (645 - 580);\n    B = 0;\n  } else if (wavelength > 645 && wavelength <= 780) {\n    R = 1;\n    G = 0;\n    B = 0;\n  }\n\n  // intensity adjustment near the vision limits\n  let intensity = 1;\n  if (wavelength >= 700) {\n    intensity = 0.3 + (0.7 * (780 - wavelength)) / (780 - 700);\n  } else if (wavelength < 420) {\n    intensity = 0.3 + (0.7 * (wavelength - 380)) / (420 - 380);\n  }\n  console.log(\"intensity:\", intensity);\n\n  return [\n    Math.round(R * intensity * 255),\n    Math.round(G * intensity * 255),\n    Math.round(B * intensity * 255)\n  ];\n};\n\n// let updateCanvas = function (fillstyle) {\n//   let canvas = document.getElementById(\"canvas\");\n//   let ctx = canvas.getContext(\"2d\");\n//   ctx.fillStyle = fillstyle;\n//   ctx.fillRect(0, 0, 800, 100);\n// };\n\nexport let wavelengthToHSL = (wavelength, strength) => {\n  let rgb = wavelengthToRGB(485);\n  return [\n    rgb2hue(rgb[0], rgb[1], rgb[2]),\n    `50%`,\n    `${normalizeStrength(strength)}%`\n  ];\n};\n"},"lineCount":null}},"error":null,"hash":"38660764c3132a8bb0a59c4b3a0a6b53","cacheData":{"env":{}}}